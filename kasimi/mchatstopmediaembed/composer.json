{
    "name": "kasimi/mchatstopmediaembed",
    "type": "phpbb-extension",
    "description": "Disables all media embedding in mChat messages and displays a regular clickable link instead.",
    "homepage": "https://kasimi.net",
    "version": "1.0.0",
    "time": "2018-04-22",
    "license": "GPL-2.0-only",
    "authors": [
        {
            "name": "kasimi",
            "email": "mail@kasimi.net",
            "homepage": "https://kasimi.net",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4.7",
        "composer/installers": "~1.0"
    },
    "extra": {
        "display-name": "mChat Stop Media Embed",
        "soft-require": {
            "phpbb/phpbb": ">=3.2.0"
        },
        "version-check": {
            "host": "kasimi.github.io",
            "directory": "/phpbb_versions",
            "filename": "mchatstopmediaembed_version.json",
            "ssl": true
        }
    }
}
